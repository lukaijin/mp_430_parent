.concat-box {
  min-height: 100%;
  background-color: #40BF6A;
}
.concat-box .concat {
  width: 650rpx;
  margin: 110rpx auto 0;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: contain;
}
.concat-box .concat .header {
  height: 270rpx;
  flex-direction: column;
}
.concat-box .concat .header .p {
  font-size: 40rpx;
  color: #333;
  font-weight: 500;
}
.concat-box .concat .header .tel {
  font-size: 34rpx;
  color: #40BF6A;
  margin-top: 35rpx;
}
.concat-box .concat .bottom {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  height: 650rpx;
  padding: 50rpx 65rpx 100rpx;
  box-sizing: border-box;
}
.concat-box .concat .bottom .p {
  font-size: 28rpx;
  color: #aaa;
}
.concat-box .concat .bottom .p:nth-child(2) {
  line-height: 50rpx;
}
.concat-box .concat .bottom .time {
  color: #333;
}
.concat-box .concat .bottom .wechat {
  font-size: 40rpx;
  color: #333;
}
