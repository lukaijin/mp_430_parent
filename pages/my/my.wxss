.my {
   width: 100%;
 }
 .my .my-in {
   padding: 0 50rpx;
   box-sizing: border-box;
 }
 .my .myinfo {
   box-sizing: border-box;
   height: 140rpx;
   width: 100%;
   display: flex;
   margin: 55rpx 0 45rpx;
   justify-content: space-between;
 }
 .my .myinfo .avatar {
   width: 140rpx;
   height: 140rpx;
   border-radius: 50%;
   position: relative;
 }
 .my .myinfo .avatar .img {
   width: 140rpx;
   height: 140rpx;
   border-radius: 50%;
   position: relative;
   z-index: 1;
 }
 .my .myinfo .nickname-box {
   display: flex;
   flex-direction: column;
   justify-content: center;
 }
 .my .myinfo .nickname-box .nickname {
   margin-bottom: 20rpx;
   font-weight: bold;
   font-size: 34rpx;
 }
 .my .myinfo .nickname-box .phone {
   font-size: 26rpx;
   color: #AAAAAA;
 }
 .my .myinfo .nickname-box .phone .icon-brush_fill {
   font-size: 30rpx;
   color: #40BF6A;
   margin-left: 10rpx;
 }
 .my .myinfo .nickname-box .phone-tips {
   font-size: 26rpx;
   color: #FF9A49;
 }
 .my .info-list .info {
   width: 100%;
   height: 122rpx;
   box-sizing: border-box;
   justify-content: space-between;
   align-items: center;
 }
 .my .info-list .info .info-name {
   display: flex;
   align-items: center;
 }
 .my .info-list .info .info-name .p {
   font-size: 30rpx;
   color: #333333;
   display: flex;
   align-items: center;
 }
 .my .info-list .info .info-name .p .iconfont {
   margin-right: 30rpx;
 }
 .my .info-list .info .iconfont {
   color: #cccccc;
   font-size: 48rpx;
 }
 .my .minProgram {
   width: 650rpx;
   height: 96rpx;
   background: #f5f5f5;
   border-radius: 48rpx;
   margin: auto;
   line-height: 96rpx;
   text-align: center;
   font-size: 30rpx;
   margin-top: 50rpx;
   margin-bottom: 130rpx;
 }
 .my .minProgram .navigator {
   width: 100%;
   height: 100%;
   background: transparent;
   margin: 0;
 }
 .my .minProgram .icons {
   margin-right: 19rpx;
 }
 